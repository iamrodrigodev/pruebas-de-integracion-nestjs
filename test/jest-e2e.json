{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testMatch": ["<rootDir>/**/*.e2e-spec.ts"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "<rootDir>/../src/**/*.ts"
  ],
  "coverageDirectory": "./coverage",
  "testEnvironment": "node",
  "coverageProvider": "v8"
}